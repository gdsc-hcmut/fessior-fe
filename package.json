{
  "name": "fessior-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rsbuild dev",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "start": "pnpm build && pnpm preview",
    "lint": "echo NOT_IMPLEMENTED",
    "lint:fix": "echo NOT_IMPLEMENTED",
    "prettier": "echo NOT_IMPLEMENTED",
    "add:img": "chmod u+x ./scripts/add-img.sh && sh ./scripts/add-img.sh",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --colors --watch",
    "test:nowatch": "yarn test --colors",
    "test:ci": "CI=true NODE_ENV=test yarn test",
    "commit": "yarn test --colors --onlyChanged && git add . && ./node_modules/.bin/cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@react-oauth/google": "^0.12.1",
    "@rsbuild/plugin-svgr": "^0.7.10",
    "@tanstack/react-query": "^5.8.2",
    "@types/aos": "^3.0.7",
    "aos": "^2.3.4",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "qr-code-styling": "^1.6.0-rc.1",
    "react": "^18",
    "react-countup": "^6.5.0",
    "react-dom": "^18",
    "react-scroll": "^1.9.0",
    "react-toastify": "^10.0.4",
    "swiper": "^11.0.5",
    "tailwind-scrollbar-hide": "^1.1.7",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@rsbuild/core": "^0.7.10",
    "@rsbuild/plugin-react": "^0.7.10",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18.3.0",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.0.1",
    "commitizen": "^4.3.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.34.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm prettier",
      "pnpm lint:fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
